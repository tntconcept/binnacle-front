{
  "extends": "react-app",
  "rules": {
    "no-console": "warn",
    "react/jsx-curly-brace-presence": [
      "warn",
      {
        "props": "never",
        "children": "never"
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-first-prop-new-line": [
      "warn",
      "multiline"
    ],
    "react/jsx-max-props-per-line": [
      "warn",
      {
        "maximum": 1
      }
    ]
  }
}