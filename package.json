    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-pwa": "0.16.4",
    "vite-plugin-svgr": "3.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.6.7"
  }
}
