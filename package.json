{
  "name": "binnacle-api-react",
  "version": "3.0.6",
  "private": true,
  "homepage": "/binnacle",
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "PUBLIC_URL=/ env-cmd -f .env.development react-scripts build",
    "build:int": "env-cmd -f .env.integration react-scripts build && npm run zip-build",
    "build:prod": "GENERATE_SOURCEMAP=false react-scripts build && npm run zip-build",
    "zip-build": "cd build; zip -r ../binnacle_front_$npm_package_version.zip * .htaccess",
    "test": "env-cmd -f .env.development react-scripts test",
    "coverage": "env-cmd -f .env.development react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext ts --ext tsx --fix",
    "check-types": "tsc -p tsconfig.json --pretty -noEmit",
    "format-all": "prettier --write \"src/**/*.{ts,tsx}\"",
    "analyze-bundle": "node analyze.js",
    "postinstall": "patch-package",
    "cypress": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@chakra-ui/core": "1.0.0-rc.7",
    "@chakra-ui/theme": "1.0.0-rc.7",
    "@types/jest": "26.0.15",
    "@types/match-sorter": "^5.0.0",
    "@types/node": "12.19.2",
    "@types/react": "16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@types/yup": "0.29.8",
    "browser-image-compression": "^1.0.12",
    "focus-visible": "^5.2.0",
    "formik": "2.2.1",
    "framer-motion": "2.9.4",
    "heroicons": "^0.4.2",
    "i18next": "19.8.3",
    "i18next-browser-languagedetector": "6.0.1",
    "immer": "7.0.14",
    "ky": "0.24.0",
    "match-sorter": "^5.0.0",
    "react": "0.0.0-experimental-4ead6b530",
    "react-dom": "0.0.0-experimental-4ead6b530",
    "react-focus-on": "^3.5.0",
    "react-i18next": "11.7.3",
    "react-ios-pwa-prompt": "1.8.4",
    "react-popper-tooltip": "2.11.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-textarea-autosize": "^8.2.0",
    "reakit": "1.3.0-alpha.3",
    "typescript": "4.0.5",
    "use-async-resource": "2.2.0",
    "yarn": "1.22.10",
    "yup": "0.29.3",
    "workbox-background-sync": "6.0.2",
    "workbox-broadcast-update": "6.0.2",
    "workbox-cacheable-response": "6.0.2",
    "workbox-core": "6.0.2",
    "workbox-expiration": "6.0.2",
    "workbox-google-analytics": "6.0.2",
    "workbox-navigation-preload": "6.0.2",
    "workbox-precaching": "6.0.2",
    "workbox-range-requests": "6.0.2",
    "workbox-routing": "6.0.2",
    "workbox-strategies": "6.0.2",
    "workbox-streams": "6.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "yarn check-types && yarn lint && yarn test --coverage --watchAll=false"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/api/interfaces/*.ts",
      "!src/serviceWorker.ts"
    ]
  },
  "coverageThreshold": {
    "global": {
      "statements": 86,
      "branches": 65,
      "functions": 70,
      "lines": 86
    }
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@testing-library/cypress": "7.0.1",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@testing-library/react-hooks": "3.4.2",
    "@testing-library/user-event": "12.1.10",
    "@types/react-responsive": "^8.0.2",
    "cypress": "5.5.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-jest-adapter": "^0.1.1",
    "cypress-plugin-tab": "^1.0.5",
    "date-fns": "^2.16.1",
    "env-cmd": "^10.1.0",
    "husky": "4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "10.5.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier-eslint-cli": "5.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-responsive": "^8.1.0",
    "react-test-renderer": "17.0.1",
    "webpack-bundle-analyzer": "3.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
