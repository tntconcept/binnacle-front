    "downshift": "7.6.0",
    "focus-visible": "5.2.0",
    "framer-motion": "10.13.1",
    "history": "5.3.0",
    "i18next": "23.2.11",
    "i18next-browser-languagedetector": "7.1.0",
    "match-sorter": "6.3.1",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "12.0.4",
    "react-focus-on": "3.5.4",
    "react-hook-form": "7.45.2",
    "react-i18next": "13.0.2",
