{
  "name": "binnacle-front",
  "author": {
    "name": "Emanuel Suca"
  },
  "version": "4.0.0",
  "private": true,
  "homepage": "/binnacle",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.production.json && vite build",
    "serve": "vite preview",
    "build:dev": "PUBLIC_URL=./ env-cmd -f .env.development npm run build",
    "build:int": "env-cmd -f .env.integration npm run build && npm run zip-build",
    "build:prod": "npm run build && npm run zip-build",
    "zip-build": "cd build; zip -r ../binnacle_front_$npm_package_version.zip * .htaccess",
    "test:unit": "TZ=Europe/Madrid env-cmd -f .env.development jest --watchAll=true",
    "test:unit:coverage": "npm run test:unit -- --watchAll=false --coverage",
    "test:unit:ci": "npm run test:unit -- --runInBand --watchAll=false --coverage",
    "test:e2e": "cypress run",
    "test:types": "tsc -p tsconfig.json --pretty -noEmit",
    "test:lint": "eslint . --ext ts --ext tsx --fix",
    "format-all": "prettier --write \"src/**/*.{ts,tsx}\"",
    "cypress": "cypress open",
    "clean": "rm -Rf build/ coverage/ node_modules/ package-lock.json"
  },
  "dependencies": {
    "@chakra-ui/react": "1.7.2",
    "@emotion/react": "11.7.0",
    "@emotion/styled": "11.6.0",
    "@heroicons/react": "1.0.5",
    "@hookform/devtools": "4.0.1",
    "@hookform/resolvers": "2.8.0",
    "@types/jest": "26.0.15",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@vitejs/plugin-react": "1.1.0",
    "axios": "0.21.1",
    "browser-image-compression": "1.0.17",
    "concurrently": "6.4.0",
    "eslint-plugin-cypress": "2.11.3",
    "focus-visible": "5.2.0",
    "framer-motion": "5.3.3",
    "history": "5.1.0",
    "i18next": "19.8.3",
    "i18next-browser-languagedetector": "6.0.1",
    "jest-mock-extended": "1.0.18",
    "match-sorter": "6.3.1",
    "mobx": "6.3.8",
    "mobx-react": "7.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "11.4.2",
    "react-focus-on": "3.5.4",
    "react-hook-form": "7.20.5",
    "react-i18next": "11.14.3",
    "react-ios-pwa-prompt": "1.8.4",
    "react-popper-tooltip": "4.3.1",
    "react-router-dom": "6.0.2",
    "react-textarea-autosize": "8.3.3",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.6.0",
    "typescript": "4.5.5",
    "vite": "2.7.1",
    "vite-plugin-env-compatible": "1.1.1",
    "vite-plugin-svgr": "0.6.0",
    "vite-tsconfig-paths": "3.3.17",
    "wait-on": "6.0.0",
    "yup": "0.32.11"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "yarn check-types && yarn lint && yarn test --coverage --watchAll=false"
    }
  },
  "coverageThreshold": {
    "global": {
      "statements": 86,
      "branches": 65,
      "functions": 70,
      "lines": 86
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.16.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@testing-library/cypress": "7.0.6",
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "16.11.12",
    "@types/react-responsive": "8.0.4",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "camelcase": "6.2.1",
    "circular-dependency-plugin": "5.2.2",
    "cypress": "6.3.0",
    "cypress-file-upload": "5.0.8",
    "cypress-jest-adapter": "0.1.1",
    "date-fns": "2.27.0",
    "downshift": "6.1.7",
    "env-cmd": "10.1.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.3",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "10.5.0",
    "prettier-eslint-cli": "5.0.1",
    "react-responsive": "8.1.0",
    "simple-progress-webpack-plugin": "2.0.0",
    "ts-jest": "27.1.1",
    "vite-plugin-pwa": "0.11.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=15.14.0",
    "npm": ">=7.7.6"
  }
}
