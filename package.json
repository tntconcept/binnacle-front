{
  "name": "binnacle-front",
  "version": "4.0.33-SNAPSHOT.0",
  "private": true,
  "homepage": "/binnacle",
  "scripts": {
    "dev": "vite",
    "dev:sw": "rimraf dev-dist && cross-env DEBUG=vite-plugin-pwa:* SW_DEV=true vite --force",
    "dev:reloadsw": "npm run build:reloadsw && serve build",
    "dev:reloadsw-claim": "npm run build:reloadclaim && serve build",
    "build": "tsc && npm run build:reloadclaim",
    "build:reloadsw": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true RELOAD_SW=true vite build",
    "build:reloadclaim": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ SOURCE_MAP=true CLAIMS=true RELOAD_SW=true vite build",
    "serve": "vite preview",
    "build:dev": "PUBLIC_URL=./ env-cmd -f .env.development npm run build",
    "build:int": "env-cmd -f .env.integration npm run build && npm run zip-int",
    "build:prod": "npm run build && npm run zip-prod",
    "zip-int": "cd build; zip -r ../binnacle_front_${npm_package_version}_int.zip * .htaccess",
    "zip-prod": "cd build; zip -r ../binnacle_front_$npm_package_version.zip * .htaccess",
    "test:unit": "TZ=Europe/Madrid env-cmd -f .env.development jest --watchAll=true",
    "test:unit:coverage": "npm run test:unit -- --watchAll=false --coverage",
    "test:unit:ci": "npm run test:unit -- --runInBand --watchAll=false --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:types": "tsc -p tsconfig.json --pretty -noEmit",
    "lint": "eslint ./src ./cypress --ext .jsx,.js,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\" \"cypress/**/*.{js,ts}\"",
    "clean": "rm -Rf build/ coverage/ node_modules/ package-lock.json",
    "echo:version": "echo $npm_package_version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@archimedes/arch": "2.2.1",
    "@chakra-ui/icons": "2.0.13",
    "@chakra-ui/react": "1.8.6",
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@heroicons/react": "1.0.6",
    "@hookform/devtools": "4.0.2",
    "@hookform/resolvers": "2.8.8",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.42",
    "@types/react-dom": "17.0.14",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "1.2.0",
    "axios": "0.26.1",
    "browser-image-compression": "1.0.17",
    "chart.js": "4.2.0",
    "concurrently": "7.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "focus-visible": "5.2.0",
    "framer-motion": "6.2.8",
    "history": "5.3.0",
    "i18next": "21.6.14",
    "i18next-browser-languagedetector": "6.1.4",
    "jest-mock-extended": "2.0.4",
    "match-sorter": "6.3.1",
    "prettier": "2.6.0",
    "qs": "6.11.0",
    "react": "17.0.2",
    "react-chartjs-2": "5.2.0",
    "react-dom": "17.0.2",
    "react-dropzone": "12.0.4",
    "react-focus-on": "3.5.4",
    "react-hook-form": "7.28.1",
    "react-i18next": "11.16.1",
    "react-ios-pwa-prompt": "1.8.4",
    "react-popper-tooltip": "4.3.1",
    "react-router-dom": "6.2.2",
    "react-textarea-autosize": "8.3.3",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.6.0",
    "typescript": "4.6.2",
    "vite": "2.7.13",
    "vite-plugin-env-compatible": "1.1.1",
    "vite-plugin-svgr": "1.1.0",
    "vite-tsconfig-paths": "3.4.1",
    "wait-on": "6.0.1",
    "yup": "0.32.11"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "coverageThreshold": {
    "global": {
      "statements": 86,
      "branches": 65,
      "functions": 70,
      "lines": 86
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.17.8",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@nabla/vite-plugin-eslint": "1.4.0",
    "@rollup/plugin-replace": "^5.0.1",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "17.0.23",
    "@types/qs": "6.9.7",
    "@types/react-responsive": "8.0.5",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "babel-plugin-transform-typescript-metadata": "0.3.2",
    "camelcase": "6.3.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "^7.0.3",
    "cypress": "12.16.0",
    "cypress-file-upload": "5.0.8",
    "cypress-jest-adapter": "0.1.1",
    "date-fns": "2.28.0",
    "downshift": "7.6.0",
    "env-cmd": "10.1.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-fetch-mock": "3.0.3",
    "lint-staged": "12.3.7",
    "prettier-eslint-cli": "5.0.1",
    "react-responsive": "8.2.0",
    "simple-progress-webpack-plugin": "2.0.0",
    "ts-jest": "27.1.3",
    "vite-plugin-pwa": "^0.13.1",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=15.14.0 <19",
    "npm": ">=7.7.6 <=9.5"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.5.0"
  }
}
